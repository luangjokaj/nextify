{
	"name": "create-nextify-app",
	"version": "0.0.2-21",
	"description": "React apps using Next.js and Emotion.",
	"keywords": [
		"nextjs",
		"next",
		"react",
		"javascript",
		"typescript",
		"ts",
		"framework",
		"emotion",
		"styles",
		"css",
		"styling",
		"css-in-js",
		"workflow"
	],
	"homepage": "https://www.nextify.me",
	"repository": "https://github.com/luangjokaj/nextify",
	"exports": "./index.js",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"export": "next export",
		"lint": "next lint",
		"type-check": "tsc",
		"type-watch": "tsc --watch",
		"format": "prettier --write \"./**/*.{js,json,ts,tsx}\" \"!.node_modules/**\" \"!.next/**\"",
		"analyze": "ANALYZE=true next build",
		"analyze:server": "BUNDLE_ANALYZE=server next build",
		"analyze:browser": "BUNDLE_ANALYZE=browser next build"
	},
	"author": "Luan Gjokaj <luan@riangle.com>",
	"license": "MIT",
	"dependencies": {
		"@emotion/babel-plugin": "^11.10.5",
		"@emotion/react": "^11.10.5",
		"@next/bundle-analyzer": "^13.1.3",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"cherry-components": "^0.0.2-21",
		"express": "^4.18.2",
		"next": "^13.1.3",
		"prettier": "^2.8.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"typescript": "^4.9.4"
	}
}
