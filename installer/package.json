{
	"name": "create-nextify-app",
	"version": "0.0.2-3",
	"description": "React apps using Next.js and Emotion.",
	"keywords": [
		"nextjs",
		"next",
		"react",
		"javascript",
		"typescript",
		"ts",
		"framework",
		"emotion",
		"styles",
		"css",
		"styling",
		"css-in-js",
		"workflow"
	],
	"homepage": "https://www.nextify.co",
	"repository": "https://github.com/luangjokaj/nextify",
	"main": "index.js",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"export": "next export",
		"lint": "eslint --ignore-path .gitignore --cache --ext .js,.jsx,.ts,.tsx ./",
		"lint:fix": "npm run lint --fix",
		"type-check": "tsc",
		"type-watch": "tsc --watch",
		"format": "prettier --write \"./**/*.{js,json,ts,tsx}\" \"!.node_modules/**\" \"!.next/**\"",
		"analyze": "ANALYZE=true next build",
		"analyze:server": "BUNDLE_ANALYZE=server next build",
		"analyze:browser": "BUNDLE_ANALYZE=browser next build"
	},
	"author": "Luan Gjokaj <luan@riangle.com>",
	"license": "MIT",
	"dependencies": {
		"@emotion/babel-plugin": "^11.3.0",
		"@emotion/eslint-plugin": "^11.2.0",
		"@emotion/react": "^11.1.5",
		"@next/bundle-analyzer": "^10.1.3",
		"@types/node": "^14.14.37",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.3",
		"@typescript-eslint/eslint-plugin": "^4.21.0",
		"@typescript-eslint/parser": "^4.21.0",
		"babel-eslint": "^10.1.0",
		"cherry-components": "^0.0.2-8",
		"eslint": "^7.23.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"express": "^4.17.1",
		"next": "^10.1.3",
		"next-compose-plugins": "^2.2.1",
		"prettier": "^2.2.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"typescript": "^4.2.4"
	}
}
