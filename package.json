		"babel-eslint": "^10.1.0",
		"eslint": "^7.14.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"express": "^4.17.1",
		"next": "^10.0.3",
		"next-compose-plugins": "^2.2.1",
		"prettier": "^2.2.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	}
